<header>
  <ul class="horizontal">
    <li><%= image_tag 'logo.png', :alt => 'logo' %></li>
    <li><%=link_to 'Dashboard',dashboard_index_path%></li>
    <li><%=link_to 'Profile',profile_path(@profile)%></li>
    <li><%=link_to 'Friends',friendships_path%></li>
    <li><%=link_to 'Activities',activities_path%></li>
  </ul>
</header>

<%= render partial: "show", locals: { profile: @profile } %>

<% @activities.each do |activity| %>
    <div>
      <%= activity.name %>
      <%= activity.acttype %>
      <%= activity.duration %>
      <%= activity.initialTime %>
      <%= activity.place %>
      <%= activity.gpx %>
      <%= activity.user.profile.name %>
    </div>
<% end %>
